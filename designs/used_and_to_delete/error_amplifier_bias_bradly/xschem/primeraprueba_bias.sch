value="

*save data for dc simulation
.save @m.xm1.m0[id]
.save @m.xm1.m0[gm]
.save @m.xm1.m0[gds]
.save @m.xm1.m0[cgg]
.save @m.xm1.m0[cdd]
.save @m.xm1.m0[vgs]
.save @m.xm1.m0[vds]
.save @m.xm1.m0[vbs]
.save @m.xm1.m0[vth]
.save @m.xm1.m0[vdsat]

.save @m.xm3.m0[id]
.save @m.xm3.m0[gm]
.save @m.xm3.m0[gds]
.save @m.xm3.m0[cgg]
.save @m.xm3.m0[cdd]
.save @m.xm3.m0[vgs]
.save @m.xm3.m0[vds]
.save @m.xm3.m0[vbs]
.save @m.xm3.m0[vth]
.save @m.xm3.m0[vdsat]

.save @m.xm2.m0[id]
.save @m.xm2.m0[gm]
.save @m.xm2.m0[gds]
.save @m.xm2.m0[cgg]
.save @m.xm2.m0[cdd]
.save @m.xm2.m0[vgs]
.save @m.xm2.m0[vds]
.save @m.xm2.m0[vbs]
.save @m.xm2.m0[vth]
.save @m.xm2.m0[vdsat]

.save @m.xm4.m0[id]
.save @m.xm4.m0[gm]
.save @m.xm4.m0[gds]
.save @m.xm4.m0[cgg]
.save @m.xm4.m0[cdd]
.save @m.xm4.m0[vgs]
.save @m.xm4.m0[vds]
.save @m.xm4.m0[vbs]
.save @m.xm4.m0[vth]
.save @m.xm4.m0[vdsat]

.save @m.xm5.m0[id]
.save @m.xm5.m0[gm]
.save @m.xm5.m0[gds]
.save @m.xm5.m0[cgg]
.save @m.xm5.m0[cdd]
.save @m.xm5.m0[vgs]
.save @m.xm5.m0[vds]
.save @m.xm5.m0[vbs]
.save @m.xm5.m0[vth]
.save @m.xm5.m0[vdsat]

.control
save all

** Simulation
op
dc Vsd 0 1.8 0.01
dc Vsg 0 1.8 0.01

** Print op
setplot op1

let id = @m.xm1.m0[id]
let gm = @m.xm1.m0[gm]
let ro = 1/@m.xm1.m0[gds]
let Vsg = -@m.xm1.m0[vgs]
let Vsd = -@m.xm1.m0[vds]
let Vbs = -@m.xm1.m0[vbs]
let Vdsat = @m.xm1.m0[vdsat]
let Vth = @m.xm1.m0[vth]
let Vov = Vsg - abs(Vth)
let varVsd = Vsd - abs(Vdsat)
let ao = gm*ro
let gmid = gm/id
let Cgg = @m.xm1.m0[cgg]
let Cdd = @m.xm1.m0[cdd]
let fT = gm/(6.283185*Cgg)

let id_3 = @m.xm3.m0[id]
let gm_3 = @m.xm3.m0[gm]
let ro_3 = 1/@m.xm3.m0[gds]
let Vgs_3 = @m.xm3.m0[vgs]
let Vds_3 = @m.xm3.m0[vds]
let Vbs_3 = @m.xm3.m0[vbs]
let Vdsat_3 = @m.xm3.m0[vdsat]
let Vth_3 = @m.xm3.m0[vth]
let Vov_3 = Vsg_3 - abs(Vth_3)
let varVsd_3 = Vsd_3 - abs(Vdsat_3)
let ao_3 = gm_3 * ro_3
let gmid_3 = gm_3 / id_3
let Cgg_3 = @m.xm3.m0[cgg]
let Cdd_3 = @m.xm3.m0[cdd]
let fT_3 = gm_3 / (6.283185 * Cgg_3)

let id_2 = @m.xm2.m0[id]
let gm_2 = @m.xm2.m0[gm]
let ro_2 = 1/@m.xm2.m0[gds]
let Vgs_2 = @m.xm2.m0[vgs]
let Vds_2 = @m.xm2.m0[vds]
let Vbs_2 = -@m.xm2.m0[vbs]
let Vdsat_2 = @m.xm2.m0[vdsat]
let Vth_2 = @m.xm2.m0[vth]
let Vov_2 = Vsg_2 - abs(Vth_2)
let varVsd_2 = Vsd_2 - abs(Vdsat_2)
let ao_2 = gm_2 * ro_2
let gmid_2 = gm_2 / id_2
let Cgg_2 = @m.xm2.m0[cgg]
let Cdd_2 = @m.xm2.m0[cdd]
let fT_2 = gm_2 / (6.283185 * Cgg_2)

let id_4 = @m.xm4.m0[id]
let gm_4 = @m.xm4.m0[gm]
let ro_4 = 1/@m.xm4.m0[gds]
let Vgs_4 = @m.xm4.m0[vgs]
let Vds_4 = @m.xm4.m0[vds]
let Vbs_4 = -@m.xm4.m0[vbs]
let Vdsat_4 = @m.xm4.m0[vdsat]
let Vth_4 = @m.xm4.m0[vth]
let Vov_4 = Vsg_4 - abs(Vth_4)
let varVsd_4 = Vsd_4 - abs(Vdsat_4)
let ao_4 = gm_4 * ro_4
let gmid_4 = gm_4 / id_4
let Cgg_4 = @m.xm4.m0[cgg]
let Cdd_4 = @m.xm4.m0[cdd]
let fT_4 = gm_4 / (6.283185 * Cgg_4)

let id_5 = @m.xm5.m0[id]
let gm_5 = @m.xm5.m0[gm]
let ro_5 = 1/@m.xm5.m0[gds]
let Vgs_5 = @m.xm5.m0[vgs]
let Vds_5 = @m.xm5.m0[vds]
let Vbs_5 = -@m.xm5.m0[vbs]
let Vdsat_5 = @m.xm5.m0[vdsat]
let Vth_5 = @m.xm5.m0[vth]
let Vov_5 = Vsg_5 - abs(Vth_5)
let varVsd_5 = Vsd_5 - abs(Vdsat_5)
let ao_5 = gm_5 * ro_5
let gmid_5 = gm_5 / id_5
let Cgg_5 = @m.xm5.m0[cgg]
let Cdd_5 = @m.xm5.m0[cdd]
let fT_5 = gm_5 / (6.283185 * Cgg_5)

let region_1 = Vsg - Vth
let region_2 = Vgs_2 - Vth_2
let region_3 = Vgs_3 - Vth_3
let region_4 = Vgs_4 -Vth_4
let region_5 = Vgs_5 -Vth_5


echo 
echo 
echo 
echo ** Datos del transistor M1
echo
print id Vsd Vsg Vth region_1 Vdsat
echo 
echo
echo 
echo ** Datos del transistor M2
echo
print id_2   Vds_2 Vgs_2 Vth_2  region_2 

echo
echo
echo
echo ** Datos del transistor M3
echo
print id_3 Vds_3 Vgs_3 Vth_3 region_3

echo ** voltajes
print volt v1 v2 v3

write primeraprueba_bias.raw

** Plots dc Vsd
setplot dc1

let id = @m.xm1.m0[id]
let gm = @m.xm1.m0[gm]
let ro = 1/@m.xm1.m0[gds]

** Plot here the Vsd curves
*plot id

** Plots dc Vsg
setplot dc2

let id = @m.xm1.m0[id]
let gm = @m.xm1.m0[gm]
let gmid = gm/id
let ro = 1/@m.xm1.m0[gds]
let fT = gm/(6.283185*@m.xm1.m0[cgg])
let ftgmid = fT*gmid
let GfT = fT*gm*ro

** Plot here the Vsg curves

*plot id

.endc
"}
